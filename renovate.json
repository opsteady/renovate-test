{
  "branchPrefix": "deps/",
  "onboarding": false,
  "repositories": ["opsteady/renovate-test"],
  "enabledManagers": ["regex"],
  "regexManagers": [
    {
      "fileMatch": ["^Dockerfile$"],
      "matchStringsStrategy": "any",
      "matchStrings": [
        "datasource=(?<datasource>.*?) depName=(?<depName>.*?) (versioning=(?<versioning>.*?)\n)ENV .*?_VERSION=(?<currentValue>.*)"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
