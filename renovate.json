{
  "username": "renovate-opsteady[bot]",
  "branchPrefix": "deps/",
  "gitAuthor": "Self-hosted Renovate Bot <93674738+renovate-opsteady[bot]@users.noreply.github.com>",
  "onboarding": false,
  "repositories": ["opsteady/renovate-test"],
  "regexManagers": [
    {
      "fileMatch": ["(^|/)component.go$"],
      "matchStringsStrategy": "any",
      "matchStrings": [
        "\\w+:\\s+\"(?<currentValue>.*?)\".+\/\/ renovate: datasource=(?<datasource>.*?)\\s+registryUrl=(?<registryUrl>.*?)\\s+depName=(?<depName>.*?)\\s+versioning=(?<versioning>.*?)\\s+"
      ]
    }
  ]
}
